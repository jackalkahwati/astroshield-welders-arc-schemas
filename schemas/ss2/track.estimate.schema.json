{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://astroshield.com/schemas/ss2/track.estimate/1.0.0",
  "title": "SS2 Track Estimate",
  "type": "object",
  "required": ["schemaVersion", "messageId", "timeGenerated", "objectId", "state", "covariance6x6", "frame"],
  "properties": {
    "schemaVersion": {"const": "1.0.0"},
    "messageId": {"type": "string"},
    "timeGenerated": {"type": "string", "format": "date-time"},
    "objectId": {"type": "string"},
    "frame": {"type": "string", "enum": ["ICRF", "ECI", "RSW", "RIC"]},
    "state": {
      "type": "object",
      "required": ["r_km", "v_kms"],
      "properties": {
        "r_km": {"type": "array", "items": {"type": "number"}, "minItems": 3, "maxItems": 3},
        "v_kms": {"type": "array", "items": {"type": "number"}, "minItems": 3, "maxItems": 3}
      },
      "additionalProperties": false
    },
    "covariance6x6": {"type": "array", "items": {"type": "number"}, "minItems": 36, "maxItems": 36}
  },
  "additionalProperties": false
}



