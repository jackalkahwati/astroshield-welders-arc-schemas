{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://astroshield.com/schemas/ss5/separation.breakup-detection/1.0.0",
  "title": "SS5 Separation Breakup Detection",
  "type": "object",
  "required": ["schemaVersion","messageId","timeGenerated","producer","eventType","objectId","confidence"],
  "properties": {
    "schemaVersion": {"const": "1.0.0"},
    "messageId": {"type": "string"},
    "timeGenerated": {"type": "string", "format": "date-time"},
    "producer": {"type": "string"},
    "eventType": {"const": "ss5.separation.breakup-detection"},
    "objectId": {"type": "string"},
    "confidence": {"type": "number", "minimum": 0, "maximum": 1},
    "provenance": {"type": "object", "properties": {"manifestId": {"type": "string"}, "gitSha": {"type": "string"}, "algVersion": {"type": "string"}}, "additionalProperties": false}
  },
  "additionalProperties": false
}

