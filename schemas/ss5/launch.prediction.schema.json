{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://astroshield.com/schemas/ss5/launch.prediction/1.0.0",
  "title": "SS5 Launch Prediction",
  "type": "object",
  "required": ["schemaVersion","messageId","timeGenerated","producer","eventType","vehicle","prediction"],
  "properties": {
    "schemaVersion": {"const": "1.0.0"},
    "messageId": {"type": "string"},
    "timeGenerated": {"type": "string","format":"date-time"},
    "producer": {"type": "string"},
    "eventType": {"const": "ss5.launch.prediction"},
    "vehicle": {"type": "string"},
    "prediction": {"type":"object","required":["windowStart","windowEnd","confidence"],"properties":{"windowStart":{"type":"string","format":"date-time"},"windowEnd":{"type":"string","format":"date-time"},"confidence":{"type":"number","minimum":0,"maximum":1}},"additionalProperties":false},
    "provenance": {"type":"object","properties":{"manifestId":{"type":"string"},"gitSha":{"type":"string"},"algVersion":{"type":"string"}},"additionalProperties":false}
  },
  "additionalProperties": false
}

