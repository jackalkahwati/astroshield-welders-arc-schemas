{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://astroshield.com/schemas/ss5/launch.detected/1.0.0",
  "title": "SS5 Launch Detected (output)",
  "type": "object",
  "required": [
    "schemaVersion",
    "messageId",
    "timeDetected",
    "producer",
    "eventType",
    "launchSite",
    "vehicle",
    "windowStart",
    "windowEnd",
    "confidence",
    "provenance"
  ],
  "properties": {
    "schemaVersion": {"const": "1.0.0"},
    "messageId": {"type": "string"},
    "timeDetected": {"type": "string", "format": "date-time"},
    "producer": {"type": "string"},
    "eventType": {"const": "ss5.launch.detected"},
    "launchSite": {"type": "string"},
    "vehicle": {"type": "string"},
    "windowStart": {"type": "string", "format": "date-time"},
    "windowEnd": {"type": "string", "format": "date-time"},
    "confidence": {"type": "number", "minimum": 0, "maximum": 1},
    "provenance": {
      "type": "object",
      "required": ["manifestId", "gitSha", "algVersion"],
      "properties": {
        "manifestId": {"type": "string"},
        "gitSha": {"type": "string"},
        "containerDigest": {"type": "string"},
        "inputMessageIds": {"type": "array", "items": {"type": "string"}},
        "algVersion": {"type": "string"}
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}



