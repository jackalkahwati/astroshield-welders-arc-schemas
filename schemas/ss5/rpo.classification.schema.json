{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://astroshield.com/schemas/ss5/rpo.classification/1.0.0",
  "title": "SS5 RPO Classification",
  "type": "object",
  "required": ["schemaVersion","messageId","timeGenerated","producer","eventType","objectId","classification","confidence"],
  "properties": {
    "schemaVersion": {"const": "1.0.0"},
    "messageId": {"type": "string"},
    "timeGenerated": {"type": "string","format":"date-time"},
    "producer": {"type": "string"},
    "eventType": {"const": "ss5.rpo.classification"},
    "objectId": {"type": "string"},
    "classification": {"type": "string"},
    "confidence": {"type": "number","minimum":0,"maximum":1}
  },
  "additionalProperties": false
}

