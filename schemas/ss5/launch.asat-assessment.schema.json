{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://astroshield.com/schemas/ss5/launch.asat-assessment/1.0.0",
  "title": "SS5 Launch ASAT Assessment",
  "type": "object",
  "required": ["schemaVersion","messageId","timeGenerated","producer","eventType","vehicle","assessment"],
  "properties": {
    "schemaVersion": {"const": "1.0.0"},
    "messageId": {"type": "string"},
    "timeGenerated": {"type": "string","format":"date-time"},
    "producer": {"type": "string"},
    "eventType": {"const": "ss5.launch.asat-assessment"},
    "vehicle": {"type": "string"},
    "assessment": {
      "type": "object",
      "required": ["capability","opportunity","risk"],
      "properties": {
        "capability": {"type":"number","minimum":0,"maximum":1},
        "opportunity": {"type":"number","minimum":0,"maximum":1},
        "risk": {"type":"number","minimum":0,"maximum":1}
      },
      "additionalProperties": false
    },
    "provenance": {"type":"object","properties":{"manifestId":{"type":"string"},"gitSha":{"type":"string"},"algVersion":{"type":"string"}},"additionalProperties":false}
  },
  "additionalProperties": false
}

